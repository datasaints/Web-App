angular.module('DataSaints').service('CountService', function($http) {

    this.getCount = function(whatToGet) {
      console.log('what to get = ' +whatToGet);
      return $http({
          url: "/datasaints/getItemCount", 
          method: "GET",
          params: 1
       }).then(function(result){
        return result.data;
       });
    };
});
